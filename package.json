{
  "private": true,
  "browserslist": [
    "last 2 versions",
    "not IE 11",
    "Firefox ESR"
  ],
  "scripts": {
    "ec": "editorconfig-checker",
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "build": "cross-env NODE_ENV=production gulp build",
    "start": "cross-env NODE_ENV=development gulp start",
    "test": "npm run econfig && npm run js:lint && npm run css:lint",
    "fix": "npm run js:fix && npm run css:fix",
    "js:lint": "eslint source/js/**/*.js",
    "js:fix": "eslint source/js/script.js --fix",
    "css:fix": "stylelint source/**/*.scss --fix"
  },
  "editorconfig-cli": [
    "source/**/*.html",
    "source/js/**/*.js",
    "source/sass/**/*.{sass,scss}"
  ],
  "devDependencies": {
    "@babel/preset-env": "7.10.4",
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "autoprefixer": "9.5.x",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.0.6",
    "browser-sync": "2.26.12",
    "clean-webpack-plugin": "3.0.0",
    "cross-env": "7.0.2",
    "del": "5.1.0",
    "editorconfig-checker": "^3.1.0",
    "eslint": "^7.4.0",
    "eslint-config-google": "^0.14.0",
    "gulp": "4.0.x",
    "gulp-csso": "3.0.1",
    "gulp-handlebars": "^5.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-rename": "1.4.0",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "gulp-svgstore": "^7.0.1",
    "gulp-twig": "^1.2.0",
    "gulp-webp": "^4.0.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^4.42.0",
    "webpack-stream": "^5.2.1"
  },
  "dependencies": {
    "focus-visible": "^5.2.0"
  }
}
